{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/screens/*": [
        "./src/screens/*"
      ],
      "@/navigation/*": [
        "./src/navigation/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@locales/*": [
        "./src/locales/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],

    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
